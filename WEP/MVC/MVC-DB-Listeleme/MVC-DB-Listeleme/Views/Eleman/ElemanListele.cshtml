
@{
    ViewBag.Title = "";
    Layout = "~/Views/_LayoutPage1.cshtml";
}

@using MVC_DB_Listeleme.Models;

<h1>Eleman Listesi</h1>
@foreach (Eleman eleman in ViewBag.ElemanCantasi)
{
    <section style="color:red; background-color:black; font-size:25px; border:3px solid blue; padding:10px; margin:10px; text-align:center;">
        <section>
            <span>Eleman Adı: @eleman.Ad</span><br />
            <span>Eleman Soyadı: @eleman.Soyad</span><br />
            <span>Eleman Şehir: @eleman.Sehir</span><br />
        </section>
    </section>
}